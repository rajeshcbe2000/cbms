<!DOCTYPE sql-map PUBLIC "-//iBATIS.com//DTD SQL Map 1.0//EN" "http://www.ibatis.com/dtd/sql-map.dtd">
<sql-map name="AdvancesProductSplAssetsMap">

	<!-- Use AdvancesProductSplAssetsTO object (JavaBean) properties as parameters for insert. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. -->
    <mapped-statement name="insertAdvancesProductSplAssetsTO" inline-parameters="true">
        INSERT INTO ADVANCES_PROD_SPLASSET
        (PROD_ID, ASSET_CATEGORY, ASSET_CATEGORY_RATEPER
        ) VALUES (
        #prodId:VARCHAR#, #assetCategory:VARCHAR#, #assetCategoryRateper:NUMERIC#)
    </mapped-statement>

	<!-- Use AdvancesProductSplAssetsTO object (JavaBean) properties as parameters for update. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. -->
    <mapped-statement name="updateAdvancesProductSplAssetsTO" inline-parameters="true">
        UPDATE ADVANCES_PROD_SPLASSET SET 
        PROD_ID = #prodId:VARCHAR#,
        ASSET_CATEGORY = #assetCategory:VARCHAR#,
        ASSET_CATEGORY_RATEPER = #assetCategoryRateper:NUMERIC#
        WHERE PROD_ID = #prodId:VARCHAR#
    </mapped-statement>

	<!-- Use AdvancesProductSplAssetsTO object (JavaBean) properties as parameters for delete. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. --> 
    <mapped-statement name="deleteAdvancesProductSplAssetsTO" inline-parameters="true">
        DELETE FROM ADVANCES_PROD_SPLASSET 
        WHERE PROD_ID = #prodId:VARCHAR#
    </mapped-statement>

	<!-- Use primitive wrapper type (e.g. Integer) as parameter and allow results to 
	     be auto-mapped results to AdvancesProductSplAssetsTO object (JavaBean) properties -->
    <result-map name="getAdvancesProductSplAssetsTOResult" class="com.see.truetransact.transferobject.product.advances.AdvancesProductSplAssetsTO">
        <property name="prodId" column="PROD_ID"/>
        <property name="assetCategory" column="ASSET_CATEGORY"/>
        <property name="assetCategoryRateper" column="ASSET_CATEGORY_RATEPER"/>
    </result-map>
    <mapped-statement name="getSelectAdvancesProductSplAssetsTO" result-map="getAdvancesProductSplAssetsTOResult" >
        SELECT * FROM ADVANCES_PROD_SPLASSET 
        WHERE PROD_ID = #value:VARCHAR#
    </mapped-statement>

</sql-map>