<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sql-map PUBLIC "-//iBATIS.com//DTD SQL Map 1.0//EN" "http://www.ibatis.com/dtd/sql-map.dtd">
<sql-map name="AdvancesProductSIMap">

	<!-- Use AdvancesProductSITO object (JavaBean) properties as parameters for insert. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. -->
    <mapped-statement name="insertAdvancesProductSITO" inline-parameters="true">
        INSERT INTO ADVANCES_PROD_SPECIALITEM
        (PROD_ID, ATM_CARD_ISSUED, CR_CARD_ISSUED, DEBIT_CARD_ISSUED, MOBILE_BANK_CLIENT, BRANCH_BANKING_ALLOWED
        ) VALUES (
        #prodId:VARCHAR#, #atmCardIssued:VARCHAR#, #crCardIssued:VARCHAR#, #debitCardIssued:VARCHAR#, #mobileBankClient:VARCHAR#, #branchBankingAllowed:VARCHAR#)
    </mapped-statement>

	<!-- Use AdvancesProductSITO object (JavaBean) properties as parameters for update. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. -->
    <mapped-statement name="updateAdvancesProductSITO" inline-parameters="true">
        UPDATE ADVANCES_PROD_SPECIALITEM SET 
        PROD_ID = #prodId:VARCHAR#,
        ATM_CARD_ISSUED = #atmCardIssued:VARCHAR#,
        CR_CARD_ISSUED = #crCardIssued:VARCHAR#,
        DEBIT_CARD_ISSUED = #debitCardIssued:VARCHAR#,
        MOBILE_BANK_CLIENT = #mobileBankClient:VARCHAR#,
        BRANCH_BANKING_ALLOWED = #branchBankingAllowed:VARCHAR#
        WHERE PROD_ID = #prodId:VARCHAR#
    </mapped-statement>

	<!-- Use AdvancesProductSITO object (JavaBean) properties as parameters for delete. 
	     Each of the parameters in the #hash# symbols is a JavaBeans property. --> 
    <mapped-statement name="deleteAdvancesProductSITO" inline-parameters="true">
        DELETE FROM ADVANCES_PROD_SPECIALITEM 
        WHERE PROD_ID = #prodId:VARCHAR#
    </mapped-statement>

	<!-- Use primitive wrapper type (e.g. Integer) as parameter and allow results to 
	     be auto-mapped results to AdvancesProductSITO object (JavaBean) properties -->
    <result-map name="getAdvancesProductSITOResult" class="com.see.truetransact.transferobject.product.advances.AdvancesProductSITO">
        <property name="prodId" column="PROD_ID"/>
        <property name="atmCardIssued" column="ATM_CARD_ISSUED"/>
        <property name="crCardIssued" column="CR_CARD_ISSUED"/>
        <property name="debitCardIssued" column="DEBIT_CARD_ISSUED"/>
        <property name="mobileBankClient" column="MOBILE_BANK_CLIENT"/>
        <property name="branchBankingAllowed" column="BRANCH_BANKING_ALLOWED"/>
    </result-map>
    <mapped-statement name="getSelectAdvancesProductSITO" result-map="getAdvancesProductSITOResult" >
        SELECT * FROM ADVANCES_PROD_SPECIALITEM 
        WHERE PROD_ID = #value:VARCHAR#
    </mapped-statement>

</sql-map>